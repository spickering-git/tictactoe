{
  "name": "tictactoe-slack",
  "description": "Uses Starbot as base to build tictactoe",
  "repository": "https://github.com/spickering-git/tictactoe-slack",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "TICTACTOE_COMMAND_TOKEN": {
      "description": "Slash command token, for the tictactoe command endpoint",
      "required": true
    },
    "SLACK_TOKEN": {
      "description": "Slack bot RTM API token",
      "required": false
    },
	"SLACK_API_TOKEN": {
      "description": "Slack WEB API token",
      "required": false
    },
    "NODE_ENV": "staging"
  },
  "image": "heroku/nodejs"
}
